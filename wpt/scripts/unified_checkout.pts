//Unified	checkout WPT script
logData	0

setCookie	https://$DOMAIN	m.pp=b
setCookie	http://$DOMAIN	m.pp=b
setCookie	https://m.stubhub.com	m.pp=b
setCookie	http://m.stubhub.com	m.pp=b

//	go to the login screen for a specific purchase
//navigate	https://$DOMAIN/checkout/mobile/Signin?ticket_id=$TICKET_ID&redir=%2Fnewcheckout%2F%3Fticket_id%3D$TICKET_ID%26quantity_selected%3D2
navigate	https://buy.stubhub.com/checkout/mobile/Signin?ticket_id=1116624588&redir=%2Fnewcheckout%2F%3Fticket_id%3D1116624588%26quantity_selected%3D2

clickAndWait	id=ticket_checkout

//	Log in
//Set Attribute Value for fields
setValue	id=loginEmail	$USR_NAME
setValue	id=loginPassword	$PASS
setValue	id=rememberMe	on
setValue	id=uniqueSessionGuid	$TICKET_ID

// Due to text area containing dummy default data we'll also need to use javascript to populate correctly
exec	document.getElementById('loginEmail').value = '$USR_NAME';
exec	document.getElementById('loginPassword').value = '$PASS';

submitForm	id=signinForm
clearCache

setCookie	https://buy.stubhub.com	m.pp=b
setCookie	http://buy.stubhub.com	m.pp=b
setCookie	https://m.stubhub.com	m.pp=b
setCookie	http://m.stubhub.com	m.pp=b

logData	1

//Now that we are signed in, lets capture the time taken to load XO step 1
navigate	https://$DOMAIN/newcheckout/?ticket_id=$TICKET_ID&quantity_selected=2

